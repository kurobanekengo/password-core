{
  "compilerOptions": {
    //"allowJs": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "lib": ["es5", "es6"],
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "declaration": true,
    "paths": {
      "@api/*": ["lib/api/*"],
      "@model/*": ["lib/model/*"],
      "@config/*": ["lib/config/*"]
    }
  },
  "exclude": [
    "node_modules"
  ]
}